                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="wishlistLabel" layoutX="226.0" layoutY="766.0" prefHeight="17.0" prefWidth="60.0" text="WISHLIST" textAlignment="CENTER">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="profileLabel" layoutX="315.0" layoutY="766.0" prefHeight="17.0" prefWidth="60.0" text="PROFILE" textAlignment="CENTER">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
      <Button fx:id="addButton" layoutX="360.0" layoutY="20.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#addButtonClicked" prefHeight="20.0" prefWidth="20.0" />
      <Button fx:id="doneButton" layoutX="360.0" layoutY="20.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#doneButtonClicked" prefHeight="20.0" prefWidth="20.0" />
      <Button fx:id="backButton" layoutX="20.0" layoutY="20.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="20.0" prefWidth="20.0" />
      <ChoiceBox fx:id="choiceBox" layoutX="20.0" layoutY="60.0" prefHeight="25.0" prefWidth="360.0" />
      <Label fx:id="chooseCatLabel" layoutX="20.0" layoutY="40.0" text="CHOOSE A CATEGORY">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="chooseNameLabel" layoutX="20.0" layoutY="110.0" text="ITEM NAME">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="nameBox" layoutX="20.0" layoutY="130.0" prefHeight="25.0" prefWidth="360.0" />
      <Label fx:id="chooseBrandLabel" layoutX="20.0" layoutY="180.0" text="BRAND">
         <font>
            <Font size="14.0" />
