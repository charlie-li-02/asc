<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="background" prefHeight="800.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.closetPageController">
    <children>
      <TreeView fx:id="treeView" layoutX="20.0" layoutY="40.0" onContextMenuRequested="#mouseClick" onMouseClicked="#mouseClick" prefHeight="650.0" prefWidth="360.0" showRoot="false" />
        <Label fx:id="homeLabel" layoutX="40.0" layoutY="766.0" prefHeight="17.0" prefWidth="60.0" text="HOME" textAlignment="CENTER">
            <font>
                <Font size="14.0" />
            </font></Label>
        <Button fx:id="homeButton" layoutX="30.0" layoutY="700.0" mnemonicParsing="false" onAction="#homeButtonPressed" prefHeight="60.0" prefWidth="60.0" />
        <Button fx:id="closetButton" layoutX="125.0" layoutY="700.0" mnemonicParsing="false" onAction="#closetButtonPressed" prefHeight="60.0" prefWidth="60.0" />
        <Button fx:id="wishlistButton" layoutX="224.0" layoutY="700.0" mnemonicParsing="false" onAction="#wishlistButtonPressed" prefHeight="60.0" prefWidth="60.0" />
        <Button fx:id="profileButton" layoutX="310.0" layoutY="700.0" mnemonicParsing="false" onAction="#profileButtonPressed" prefHeight="60.0" prefWidth="60.0" />
