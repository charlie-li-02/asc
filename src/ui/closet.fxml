      </Label>
      <TextField fx:id="brandBox" layoutX="20.0" layoutY="200.0" prefHeight="25.0" prefWidth="360.0" />
      <Label fx:id="chooseSizeLabel" layoutX="20.0" layoutY="250.0" text="SIZE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="sizeBox" layoutX="20.0" layoutY="270.0" prefHeight="25.0" prefWidth="360.0" />
      <Label fx:id="choosePriceLabel" layoutX="20.0" layoutY="320.0" text="PRICE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="priceBox" layoutX="20.0" layoutY="340.0" prefHeight="25.0" prefWidth="360.0" />
      <Label fx:id="chooseImageLabel" layoutX="20.0" layoutY="390.0" text="ITEM PICTURE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="chooseImageButton" layoutX="20.0" layoutY="410.0" mnemonicParsing="false" onAction="#chooseImageButtonClicked" prefHeight="25.0" prefWidth="360.0" text="SELECT AN IMAGE">
         <font>
            <Font size="14.0" />
         </font></Button>
      <ImageView fx:id="imagePreview" fitHeight="200.0" fitWidth="200.0" layoutX="100.0" layoutY="480.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="imagePreviewLabel" layoutX="20.0" layoutY="460.0" text="PREVIEW">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
